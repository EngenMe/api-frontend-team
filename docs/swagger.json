                },
                "refresh": {
                    "$ref": "#/definitions/dto.TokenPair"
                }
            }
        },
        "dto.RegisterRequest": {
            "type": "object",
            "required": [
                "email"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
